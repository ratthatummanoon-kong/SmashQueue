# SmashQueue Root Environment Configuration
# Copy this file to .env and fill in your values

# ===================
# Database
# ===================
DB_USER=postgres
DB_PASSWORD=your_secure_password_here
DB_NAME=smashqueue
DB_PORT=5432

# ===================
# Backend
# ===================
SERVER_PORT=8080
AUTH_SECRET_KEY=smashqueue-secret-key-change-in-production-32bytes!

# ===================
# Frontend
# ===================
FRONTEND_PORT=3000
NEXT_PUBLIC_API_URL=http://localhost:8080/api

# ===================
# CORS
# ===================
CORS_ORIGIN=http://localhost:3000
